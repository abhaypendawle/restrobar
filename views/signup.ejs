<html>
<head>
    <title>Signup</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet"
          href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet"
          href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
    <script
            src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
</head>
<body background="..\images\bg.jpg">
<div><img src="..\images\logo.png" ></div>

<form action="/afterSignUp" method="post" class="afterSignUp">
    <div class="panel panel-primary" style=" margin-left: 685px; margin-top: -20px; width: 500; border-color: black; opacity: 0.8">
    <div class="panel-heading" style="background-image: linear-gradient(to bottom,black 0,black 100%)">SignUp</div>
    <div class="panel-body">
        <form class="form-horizontal" role="form">
            <div class="form-group  usernameDiv">
               <!-- <label class="inputFirstName" class="col-sm-2 control-label pull-left">Enter your First Name</label>-->
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="inputFirstname" id="inputFirstname" placeholder="Firstname" style="margin-top: 10px; margin-left: 33">
                </div>
            </div>
            <div class="form-group  usernameDiv">
               <!-- <label class="inputLastName" class="col-sm-2 control-label pull-left">Enter your Last Name</label>-->
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="inputLastname" id="inputLastname" placeholder="Lastname" style="margin-left: 33;margin-top: 10px">
                </div>
            </div>

            <div class="form-group  usernameDiv">
                <!--<label class="inputUsername" class="col-sm-2 control-label pull-left">Enter your Email id</label>-->
                <div class="col-sm-10">
                    <input type="email" class="form-control" name="inputUsername" id="inputUsername" placeholder="Email Id" style="margin-left: 33;margin-top: 10px">
                </div>
            </div>
            <div class="form-group  usernameDiv">
                <!-- <label for="inputPassword">Password</label>-->
                 <div class="col-sm-10">
                     <input type="password" class="form-control" name="inputPassword" id="inputPassword" placeholder="Password" style="margin-left: 33;margin-top: 10px">
                 </div>
             </div>

             <div class="form-group ">
                 <div class="pull-right signUp" style="margin-right: 60px">
                     <button type="submit" class="btn btn-primary" style="margin-left: 50; margin-top: 10; background-image: linear-gradient(to bottom,black 0,gray 100%)">Sign up</button>
                 </div>
             </div>
         </form>
     </div>
     </div>
 </form>

 </body>
 </html>